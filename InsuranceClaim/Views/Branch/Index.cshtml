@model  IEnumerable<Insurance.Domain.Branch>
@{
    ViewBag.Title = "Index";
}


<div class="productsave-section">
    <div class="container">
        <div class="row">
            <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6 products-top">
                <div class="head-title">
                    <h2>Branch List</h2>
                </div>
            </div>
        </div>

        <div class="row">
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 add-btn">
                <div class="">
                    @Html.ActionLink("Create New", "Create")
                </div>
            </div>
        </div>

   
        <div class="product-table-grid">
            <table class="table">
                <tr>
                    <th>
                        Branch
                    </th>
                    <th>Action</th>
                </tr>

                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.BranchName)
                        </td>
                        <td>
                            @Html.ActionLink("Edit", "Edit", new { id = item.Id }) 
                            @*@Html.ActionLink("Details", "Details", new { id = item.Id })*@ 
                            @*@Html.ActionLink("Delete", "Delete", new { id = item.Id })*@
                        </td>
                    </tr>
                }
            </table>
        </div>

    </div>


  



</div>






























